{
  "name": "things-scene-stomp",
  "version": "0.1.1",
  "description": "An element providing a starting point for your own reusable Polymer elements.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-stomp.js src/index.js & polymer serve",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "build": "browserify src/index.js -o things-scene-stomp.js && uglifyjs things-scene-stomp.js -o things-scene-stomp-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-stomp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-stomp/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-stomp#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "uglify-js": "^3.0.17",
    "watchify": "^3.3.1",
    "node-canvas": "Automattic/node-canvas"
  },
  "dependencies": {
    "sockjs-client": "^1.1.4",
    "stompjs": "^2.3.3"
  },
  "false": {}
}
